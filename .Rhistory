resp <- matrix(sample(c("correct", paste0("false", 1:3)), size = q*n, prob = c(.7, .1,.1, .1), replace = TRUE), ncol = q, nrow = n)
resp == "correct"
grades <- rowMeans(resp == "correct")
grades
grades <- rowMeans(resp == "correct")*10
grades
mean(grades)
sd(grades)
